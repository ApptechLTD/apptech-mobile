<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:mgwt="urn:import:com.googlecode.mgwt.ui.client.widget" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
		font-weight: bold;
		}
		.label {
		float: left;
		width:85px;
		text-align: right;
		color: grey;
		margin-right: 5px;
		}
		.divMandatory {
		float: left;
		line-height: 44px;
		text-align: center;
		padding-top: 10px;
		}

		.divMandatory>div {
		height: 44px;
		vertical-align:
		middle;
		}
	</ui:style>
	<g:HTMLPanel>
		<mgwt:WidgetList round="true" ui:field="incomeWidget">
			<g:HorizontalPanel ui:field="incomePanel">
				<g:HTML addStyleNames="{style.label}">Company Name:</g:HTML>
				<mgwt:MTextBox ui:field="companyName"></mgwt:MTextBox>
				<g:HTML addStyleNames="{style.label}">Income:</g:HTML>
				<mgwt:MTextBox ui:field="income"></mgwt:MTextBox>
			</g:HorizontalPanel>
		</mgwt:WidgetList>
	</g:HTMLPanel>
</ui:UiBinder> 